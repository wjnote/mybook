{
  "title": "wujun's Note",
  "author": "wujun",
  "description": "select * from learn",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./resouce/style/normalize.8.0.1.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {},
  "plugins": [
    "-sharing",
    "splitter",
    "expandable-chapters-small",  // 可扩展导航章节
    "favicon",
    "back-to-top-button",
    "simple-page-toc",  // 生成本页目录
    "page-toc-button"  //  悬浮目录
  ],
  "pluginsConfig": {
    "simple-page-toc": {
        "maxDepth": 3,
        "skipFirstH1": true
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "github-buttons": {
      "buttons": [{
        "user": "wjnote",
        "repo": "mybook",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "favicon": {
      "shortcut": "./resouce/imgs/favicon.ico",
      "bookmark": "./resouce/imgs/favicon.ico"
    }
  }
}